{
  "name": "@gondel/plugin-angular",
  "version": "0.0.7",
  "description": "Gondel Plugin to boot angular apps",
  "bugs": "https://github.com/namics/gondel/issues",
  "homepage": "https://github.com/namics/gondel",
  "repository": "https://github.com/namics/gondel.git",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Jan Nicklas",
  "license": "MIT",
  "scripts": {
    "pretest": "run-s fixtures:build",
    "test": "jest",
    "build:clean": "rimraf dist",
    "build:esm": "tsc --project ./tsconfig.json --declaration --esModuleInterop --declarationDir dist/ --sourceMap --outDir dist/",
    "build": "npm-run-all build:clean build:esm",
    "fixtures:build": "cd test/fixtures/app && run-p build"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "peerDependencies": {
    "@gondel/core": "*",
    "@angular/core": "*",
    "@angular/compiler": "*",
    "@angular/common": "*",
    "@angular/platform-browser-dynamic": "*"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/webpack-env": "1.13.6",
    "jest": "23.6.0",
    "@angular/platform-browser-dynamic": "7.0.4",
    "@angular/platform-browser": "7.0.4",
    "@gondel/core": "^0.0.7",
    "@angular/compiler": "7.0.4",
    "@angular/common": "7.0.4",
    "@angular/core": "7.0.3",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "typescript": "3.0.3",
    "ts-jest": "23.10.4",
    "tslib": "1.9.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    },
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
